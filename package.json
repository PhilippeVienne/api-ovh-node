{
  "author": "uriel Chemouni",
  "description": "Api Ovh helper",
  "version": "1.2.0",
  "private": true,
  "workspaces": [
    "packages-*/*",
    "kimsufi-*/*",
    "soyoustart-*/*",
    "common",
    "sample"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/debounce": "^1.2.0",
    "@types/node": "^13.5.0",
    "lerna": "^3.20.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.5"
  },
  "publishConfig": {
    "access": "false"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UrielCh/api-ovh-node.git"
  },
  "scripts": {
    "clean": "npm run clean0 && npm run clean1 && npm run clean2 && npm run clean3 && npm run clean4 && npm run clean5",
    "clean0": "rimraf ./code/*/index.d.ts ./code/*/index.js",
    "clean1": "rimraf ./packages-*/*/yarn-error.log ./packages-*/*/index.d.ts   ./packages-*/*/index.js ./code/*/index.d.ts ./code/*/index.js",
    "clean2": "rimraf ./kimsufi-*/yarn-error.log    ./kimsufi-*/*/index.d.ts    ./kimsufi-*/*/index.js",
    "clean3": "rimraf ./soyoustart-*/yarn-error.log ./soyoustart-*/*/index.d.ts ./soyoustart-*/*/index.js",
    "clean4": "rimraf **/.cache",
    "clean5": "lerna clean -y",
    "drop-cache": "rimraf ./*/*/.cache",
    "install-all": "lerna exec --stream -- npm i --cache ./.cache",
    "bootstrap": "lerna bootstrap --concurrency=1",
    "build": "lerna run build",
    "do-publish": "lerna clean -y && npm run install-all && npm run drop-cache && lerna bootstrap --concurrency=1 && lerna run build && lerna publish"
  },
  "dependencies": {},
  "name": "api-ovh-node"
}

{
  "name": "ovh-bill-importer",
  "version": "1.1.1",
  "description": "Download and organize all your ovh.com / OVHcloud bills",
  "main": "index.js",
  "keywords": [
    "ovh",
    "OVHcloud",
    "billing",
    "download",
    "VAT"
  ],
  "typings": "index.d.ts",
  "license": "MIT",
  "author": "Uriel Chemouni <uchemouni@gmail.com>",
  "bin": {
    "ovh-bill-importer": "./bin/ovh-bill-importer"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UrielCh/api-ovh-node.git"
  },
  "bugs": "https://github.com/UrielCh/api-ovh-node/issues",
  "scripts": {
    "test": "ts-node index.ts --max-age=0Y --split=month --api=eu --dest=../../../invoices --token ../../../token-bill-importer.json",
    "build": "tsc -p .",
    "build:watch": "tsc -p . --watch",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@ovh-api-ca/me": "^3.1.2",
    "@ovh-api-us/me": "^3.1.2",
    "@ovh-api/api": "^3.1.1",
    "@ovh-api/common": "^3.1.1",
    "@ovh-api/me": "^3.1.2",
    "bluebird": "^3.7.2",
    "commander": "^5.1.0",
    "fs-extra": "^9.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/fs-extra": "^8.1.0",
    "@types/node-fetch": "^2.5.7",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "files": [
    "bin"
  ]
}
